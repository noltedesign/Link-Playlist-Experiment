=haml :'partials/intro'

      
-@main_category.each do |feed|
  .cell.feed.column
    .cell.fit.title
      %h5.cat-title.cell
        %a{:title => feed.feed.feed_title, :href => feed.feed.feed_link, :class => 'feed-link'}
          =feed.feed.feed_title
    
    .cell.feed-scroll
      =haml :'partials/feed', :locals => {:feedin => feed.feed}