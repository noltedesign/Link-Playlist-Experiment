=@userfeed.feeds.each do |feedin|  
  %div.cell.feed
    %h5
      =feedin.feed_title
      %span
        (
        =feedin.feed_items.count
        )
    
    %ul
      -feedin.feed_items.order(published_on: :desc).each do |entry|
        %li
          %a{:title => remove_html_tags( entry.summary ), :href => entry.item_url, :target => 'blank', :class => 'tooltip'}
            =entry.title
