-@userfeed.user_feeds.order(feed_order: :asc).each do |feedin|  
  %div.cell.feed
    %h5
      -if feedin.feed.feed_type == "pinterest"
        %img{:src => "/img/pinterest.svg", :alt => "pinterest", :class => "pinterest-logo"} 
      =feedin.feed.feed_title
      %span
        (
        =feedin.feed.feed_items.count
        )
    
    =haml :'partials/feed', :locals => {:feedin => feedin.feed}